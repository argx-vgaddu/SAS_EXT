{
  "name": "sas-dataset-viewer",
  "displayName": "SAS Dataset Viewer",
  "description": "View, filter, and analyze SAS7BDAT dataset files directly in VS Code with powerful features like WHERE clause filtering, variable selection, and pagination",
  "version": "1.0.0",
  "publisher": "YOUR-PUBLISHER-ID",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e3a5f",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR-USERNAME/sas-dataset-viewer"
  },
  "homepage": "https://github.com/YOUR-USERNAME/sas-dataset-viewer#readme",
  "bugs": {
    "url": "https://github.com/YOUR-USERNAME/sas-dataset-viewer/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Data Science",
    "Viewers",
    "Other"
  ],
  "keywords": [
    "sas",
    "sas7bdat",
    "dataset",
    "data viewer",
    "statistics"
  ],
  "activationEvents": [
    "onCustomEditor:sasDatasetViewer.sas7bdat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "sasDatasetViewer.sas7bdat",
        "displayName": "SAS Dataset Viewer",
        "selector": [
          {
            "filenamePattern": "*.sas7bdat"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "sasDatasetViewer.openDataset",
        "title": "Open SAS Dataset",
        "category": "SAS"
      }
    ],
    "configuration": {
      "title": "SAS Dataset Viewer",
      "properties": {
        "sasDatasetViewer.enableDebugLogging": {
          "type": "boolean",
          "default": false,
          "description": "Enable debug logging for the SAS Dataset Viewer extension"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}